package server;

private String id; // 유닛 식별자 추가

public String getId() {
    return id;
}

@Override
public String toString() {
    return "UnitState{" +
            "id='" + id + '\'' +
            ", unitType='" + unitType + '\'' +
            ", teamSide='" + teamSide + '\'' +
            ", x=" + x +
            ", y=" + y +
            ", hp=" + hp +
            '}';
}
